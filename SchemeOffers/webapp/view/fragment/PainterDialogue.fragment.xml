<core:FragmentDefinition
	
    xmlns:m="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core"
    xmlns="sap.ui.table"
    	xmlns:u="sap.ui.unified">
	  <m:Dialog id="idP" showHeader="false"  contentWidth="700px">
     

       
           	<Table id="idPainterDialog"
              
				rows="{oModelControl>/ofragmentModel}"
				selectionMode="MultiToggle"
				visibleRowCount="7"
				paste="onPaste"
				ariaLabelledBy="title">
				<extension>
					<m:OverflowToolbar style="Clear">
						<m:Title id="title" text="Products"/>
					</m:OverflowToolbar>
				</extension>
				<columns>
					<Column width="11rem">
						<m:Label text="Membership ID" />
						<template>
							<m:Text text="{oModelControl>MembershipID}" wrapping="false" />
						</template>
					</Column>
					<Column width="8rem">
						<m:Label text="Name" />
						<template>
							<m:Text text="{oModelControl>Name}" wrapping="false" />
						</template>
					</Column>
					<Column width="8rem" hAlign="End">
						<m:Label text="Mobile Number" />
						<template>
								<m:Text text="{oModelControl>MobileNumber}" wrapping="false" />
						</template>
					</Column>
				
				
					<Column width="9rem">
						<m:Label text="Zone" />
						<template>
								<m:Text text="{oModelControl>Zone}" wrapping="false" />
						</template>
					</Column>
					
					<Column width="9rem">
						<m:Label text="Division" />
						<template>
							<m:Text text="{oModelControl>Division}" wrapping="false" />
						</template>
					</Column>
					<Column width="9rem">
						<m:Label text="Depot" />
						<template>
								<m:Text text="{oModelControl>Depot}" wrapping="false" />
						</template>
					</Column>

                <Column width="9rem">
						<m:Label text="Painter Type" />
						<template>
							<m:Text text="{oModelControl>PainterType}" wrapping="false" />
						</template>
					</Column>


                    <Column width="9rem">
						<m:Label text="Painter ArcheType" />
						<template>
							<m:Text text="{oModelControl>PainterArcheType}" wrapping="false" />
						</template>
					</Column>
				
				
					
					
					
						


                  

				</columns>
			</Table>
        
     
          <m:buttons>
            <m:Button text="Cancel" type="Ghost" press="onSavePaitnerClose" />
            <m:Button  text="Save" type="Ghost" press="onSavePaitner"/>
           
        </m:buttons>
        
   
    </m:Dialog>
</core:FragmentDefinition>
